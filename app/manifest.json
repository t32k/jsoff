{
  "name": "jsoff",
  "version": "2.1.0",
  "manifest_version": 2,
  "description": "A chrome extension to disable JavaScript with one click.",
  "icons": {
    "16": "images/icon-32.png",
    "128": "images/icon-256.png"
  },
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-38.png",
      "38": "images/icon-76.png"
    },
    "default_title": "JavaScript: ON"
  },
  "permissions": ["contentSettings", "tabs"]
}
