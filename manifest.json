{
   "name": "jsoff",
   "description": "One click JavaScript off extension.",
   "version": "1.1.0",
   "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
   },
   "browser_action": {
      "default_icon": "img/badge_on.png",
      "default_title": "OFF"
   },
   "background" : {
    "scripts": ["background.js"]
   },
   "permissions": [ "contentSettings" ],
   "manifest_version": 2
}